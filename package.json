  },
  "homepage": "https://github.com/oi-contrib/FileSystem",
  "dependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/serve-static": "^4.0.1",
    "compressing": "^1.10.0",
    "devby": "^0.7.0",
    "nodemailer": "^6.9.13",
    "reflect-metadata": "^0.1.13",
